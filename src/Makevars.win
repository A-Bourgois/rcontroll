ARCH=$(PROCESSOR_ARCHITECTURE)
VERSION=2.7

PKG_CPPFLAGS= -I../windows/gsl-${VERSION}/include
PKG_LIBS= -O3 -W -fsanitize=undefined -L../windows/gsl-${VERSION}/lib${R_ARCH}${CRT} -lgsl -lgslcblas

all: clean winlibs

clean:
#	rm -f $(OBJECTS) $(SHLIB)

winlibs:
	"${R_HOME}/bin${R_ARCH_BIN}/Rscript.exe" --vanilla "../tools/winlibs.R" ${VERSION}

.PHONY: all winlibs clean